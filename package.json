{
  "name": "norman",
  "version": "0.1.0",
  "description": "A cool way to log roast information",
  "private": true,
  "author": "Steve Davis <steve@celery.club>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/celeryclub/norman.git"
  },
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "migrate:dev": "TS_NODE_PROJECT=server/tsconfig.json node -r ts-node/register server/scripts/migrate.ts",
    "server:watch": "TS_NODE_PROJECT=server/tsconfig.json nodemon --config ./server/nodemon.json server/app.ts",
    "client:watch": "webpack --mode=development --config ./client/webpack.config.js --watch"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mysql2": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.2",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "html-webpack-plugin": "^5.4.0",
    "nodemon": "^2.0.14",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1"
  }
}
